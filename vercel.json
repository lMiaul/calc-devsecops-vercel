{
  "version": 2,
  "builds": [
    {
      "src": "api/calc.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/calc.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
